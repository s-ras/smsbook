{
	"expo": {
		"name": "SMSBook",
		"slug": "SMSBook",
		"version": "1.1.0",
		"orientation": "portrait",
		"scheme": "smsbook",
		"userInterfaceStyle": "automatic",
		"android": {
			"icon": "./src/assets/images/icon.png",
			"adaptiveIcon": {
				"monochromeImage": "./src/assets/images/adaptive-icon.png",
				"foregroundImage": "./src/assets/images/icon.png",
				"backgroundColor": "#222d3d"
			},
			"permissions": [
				"SEND_SMS",
				"READ_SMS"
			],
			"package": "com.sras.smsbook",
			"splash": {
				"image": "./src/assets/images/splash.png",
				"resizeMode": "contain",
				"backgroundColor": "#f5f5f7",
				"dark": {
					"image": "./src/assets/images/splash.png",
					"resizeMode": "contain",
					"backgroundColor": "#222d3d"
				}
			}
		},
		"plugins": [
			"expo-router",
			[
				"expo-font",
				{
					"fonts": [
						"./src/assets/fonts/Vazirmatn-UI-ExtraBold.ttf",
						"./src/assets/fonts/Vazirmatn-UI-Medium.ttf",
						"./src/assets/fonts/Vazirmatn-UI-Regular.ttf",
						"./src/assets/fonts/Vazirmatn-UI-Thin.ttf"
					]
				}
			],
			"expo-updates"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"eas": {
				"projectId": "975bd8f0-e649-4ef0-b758-1b0937d58782"
			}
		}
	}
}